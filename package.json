  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js --node-args='--max_old_space_size=8192'",
    "dev": "set NODE_OPTIONS=--max-old-space-size=8192 && nodemon server.js --ignore dist/ --ignore client/ --ignore server/mock/* --ignore mock/* --max-old-space-size=8192",
    "devins": "nodemon --inspect server.js",
    "webpack": "webpack --progress -p",
    "watch": "webpack -d --watch --progress -p",
    "build": "set NODE_ENV=production && sudo rm -rf dist/ && webpack --progress -p --info-verbosity",
    "mount": "sudo git pull && yarn install && npm run build && pm2 reload social"
  },
